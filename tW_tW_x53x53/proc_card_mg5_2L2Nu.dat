set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model sm
define p = g u c d s u~ c~ d~ s~
define j = g u c d s u~ c~ d~ s~
import model STP_UFO_freeWidth
define Topo = x53 x53~
define bb = b b~
define tt = t t~
define WW = w+ w-
define vla = ve vm vt ve~ vm~ vt~
define kk = e+ mu+ ta+ e- mu- ta-
define all =  g a ve vm vt ve~ vm~ vt~ u c t d s b u~ c~ t~ d~ s~ b~ e- mu- ta- e+ mu+ ta+
define ferm =  ve vm vt ve~ vm~ vt~ u c d s b u~ c~ d~ s~ b~ e- mu- ta- e+ mu+ ta+
generate    p p > Topo Topo , ( Topo  > tt  kk vla ,  tt > kk vla bb  )
output tW_tW_x53x53
